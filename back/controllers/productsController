const producto = require('../models/productos')


exports.getProducts = (req, res, next) => {

    res.status(200).json({
        success: true,
        message: "En esta ruta podra ver todos sus productos"
    })

}

exports.newProducto = async(req, res, nex) =>{
    const product = await producto.create(req.body);

    res.status(201).json({
        success: true,
        product
    })
}